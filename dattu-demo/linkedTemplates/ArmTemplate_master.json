{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dattu-demo"
		},
		"LS_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS'"
		},
		"LS_ADLS_2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_2'"
		},
		"LS_ASQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQL'"
		},
		"LS_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dattuadls.dfs.core.windows.net/"
		},
		"LS_ADLS_2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dattugen2.dfs.core.windows.net/"
		},
		"LS_ADLS_KEY_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dattugen2.dfs.core.windows.net"
		},
		"LS_AKV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dattu.vault.azure.net/"
		},
		"LS_ASQL_KEY_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlkey"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dattu-demo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_accountKey": {
						"value": "[parameters('LS_ADLS_accountKey')]"
					},
					"LS_ADLS_2_accountKey": {
						"value": "[parameters('LS_ADLS_2_accountKey')]"
					},
					"LS_ASQL_connectionString": {
						"value": "[parameters('LS_ASQL_connectionString')]"
					},
					"LS_ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_url')]"
					},
					"LS_ADLS_2_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_2_properties_typeProperties_url')]"
					},
					"LS_AKV_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AKV_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dattu-demo_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_KEY_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_KEY_properties_typeProperties_url')]"
					},
					"LS_ASQL_KEY_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_KEY_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dattu-demo_ArmTemplate_0')]"
			]
		},
		{
			"name": "dattu-demo_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dattu-demo_ArmTemplate_1')]"
			]
		}
	]
}